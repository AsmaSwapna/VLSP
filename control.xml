<SimOptions>
  <GlobalController>
     <Port>8888</Port>
     <StartLocalControllers>true</StartLocalControllers>
  </GlobalController>
  <LocalController>
     <Name>sepulchrave</Name>
     <Port>10000</Port>
     <LowPort>11001</LowPort>
     <HighPort>12000</HighPort>
     <MaxRouters>13</MaxRouters>
  </LocalController>
  <LocalController>
     <Name>sepulchrave</Name>
     <Port>20000</Port>
     <LowPort>21001</LowPort>
     <HighPort>22000</HighPort>
     <MaxRouters>13</MaxRouters>
  </LocalController>

  <EventEngine>
     <Name>Probabilistic</Name>
     <EndTime>3600</EndTime>
     <Parameters>probdists.xml</Parameters>
  </EventEngine>
  <RouterOptions>
      routeroptions.xml
  </RouterOptions>
      <Output>
        <File>out.net</File>
        <When>End</When>
        <Type>Network</Type>
    </Output>
    <Output>
        <File>summary.out</File>
        <When>Interval</When>
        <Time>10000</Time>
        <Type>Summary</Type>
    </Output>
    <Output>
        <File>traffic.agg</File>
        <When>Interval</When>
	<Time>10000</Time>
	<Type>Traffic</Type>
        <Parameter>Aggregate</Parameter>
    </Output>
</SimOptions>
