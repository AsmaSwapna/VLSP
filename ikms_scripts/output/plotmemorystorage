echo "Creating memory storage figure"

counter=2
result="`cat scenario`/$1.txt Memory_State Time Memory_State"

for a in `cat range`
do
        rangetype="`cat rangetype`_${a}"
        result="${result} ${rangetype} $counter"
        let counter+=1
done

echo $result

./plotmemorystoragefigure $result

./createpicture `cat scenario`/$1
