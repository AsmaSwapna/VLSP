echo "Creating memory storage figure"

counter=2
result="`cat scenario`/memorystorage.txt Memory_State Time Memory_State"

./memorystorage > `cat scenario`/memorystorage.txt

for a in `cat range`
do
        rangetype="`cat rangetype`_${a}"
        result="${result} ${rangetype} $counter"
        let counter+=1
done

echo $result

./plotmemorystoragefigure $result

./createpicture memorystorage
