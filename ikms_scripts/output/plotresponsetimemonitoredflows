echo "Creating response time for monitored flows figure"

counter=2
result="`cat scenario`/responsetimemonitoredflows.txt Average_Response_Time_(Selected_Flows) Time Response_Time"

./responsetimemonitoredflows > `cat scenario`/responsetimemonitoredflows.txt

for a in `cat range`
do
        rangetype="`cat rangetype`_${a}"
        result="${result} ${rangetype} $counter"
        let counter+=1
done

echo $result

./plotresponsetimemonitoredflowsfigure $result

./createpicture responsetimemonitoredflows
