echo "Creating freshness figure"

counter=2
result="`cat scenario`/freshness.txt Average_Information_Freshness Time Information_Freshness"

./freshness > `cat scenario`/freshness.txt

for a in `cat range`
do
        rangetype="`cat rangetype`_${a}"
        result="${result} ${rangetype} $counter"
        let counter+=1
done

echo $result

./plotfreshnessfigure $result

./createpicture freshness
