echo "Creating freshness figure"

counter=2
result="`cat scenario`/$1.txt Average_Information_Freshness Time Information_Freshness"

for a in `cat range`
do
        rangetype="`cat rangetype`_${a}"
        result="${result} ${rangetype} $counter"
        let counter+=1
done

echo $result

./plotfreshnessfigure $result

./createpicture `cat scenario`/$1
