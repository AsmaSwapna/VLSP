echo "Creating cpuload figure"

counter=2
result="`cat scenario`/cpuload.txt Average_CPU_Load Time CPU_Load"

./cpuload > `cat scenario`/cpuload.txt

for a in `cat range`
do
        rangetype="`cat rangetype`_${a}"
        result="${result} ${rangetype} $counter"
        let counter+=1
done

echo $result

./plotcpuloadfigure $result

./createpicture cpuload
