echo "Creating freshness for selected flows figure"

counter=2
result="`cat scenario`/freshnessmonitoredflows.txt Average_Information_Freshness_(Selected_Flows) Time Information_Freshness"

./freshnessmonitoredflows > `cat scenario`/freshnessmonitoredflows.txt

for a in `cat range`
do
        rangetype="`cat rangetype`_${a}"
        result="${result} ${rangetype} $counter"
        let counter+=1
done

echo $result

./plotfreshnessmonitoredflowsfigure $result

./createpicture freshnessmonitoredflows
