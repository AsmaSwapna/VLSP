echo "Creating maxenergy figure"

counter=2
result="`cat scenario`/maxenergy.txt Maximum_Energy_Consumed Time Maximum_Energy_Consumed"

./maxenergy > `cat scenario`/maxenergy.txt

for a in `cat range`
do
        rangetype="`cat rangetype`_${a}"
        result="${result} ${rangetype} $counter"
        let counter+=1
done

echo $result

./plotmaxenergyfigure $result

./createpicture maxenergy
